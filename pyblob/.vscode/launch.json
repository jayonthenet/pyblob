  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: Flask",
      "type": "debugpy",
      "request": "launch",
      "module": "flask",
      "env": {
        "FLASK_APP": "src/pyblob.py",
        "FLASK_DEBUG": "1",
        "BLOB_ACC_NAME": "jayblobexjayblobex",
        "BLOB_CONTAINER_NAME": "jayblobexjayblobex"
      },
      "args": [
        "run",
        "--no-debugger",
        "--no-reload"
      ],
      "jinja": true,
      "autoStartBrowser": true
    }
  ]
}